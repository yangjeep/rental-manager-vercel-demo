{
  "name": "rentals-demo",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html --view",
    "lighthouse:baseline": "node scripts/generate-lighthouse-baseline.js",
    "cf:build": "npx @cloudflare/next-on-pages@1 --experimental-minify",
    "cf:preview": "npx wrangler pages dev .vercel/output/static --compatibility-date=2024-12-18"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.20.7",
    "next": "14.2.33",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "@types/node": "24.10.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.11",
    "lighthouse": "^13.0.1",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.13",
    "typescript": "5.4.5"
  }
}
