# Google Sheets → Apps Script 部署 URL
NEXT_PUBLIC_LISTINGS_URL=https://script.google.com/macros/s/XXXX/exec

# 页面重新生成（ISR）时间，单位：秒
REVALIDATE_SECONDS=60

# Demo 环境用户名/密码（Basic Auth）
DEMO_USER=demo
DEMO_PASS=demo123

# 手动触发刷新接口 /api/revalidate?secret=changeme
REVALIDATE_SECRET=changeme

# Demo 阶段禁止搜索引擎收录
DEMO_NOINDEX=true

# HubSpot 表单嵌入（如果暂时不用，可留空）
NEXT_PUBLIC_HUBSPOT_PORTAL_ID=
NEXT_PUBLIC_HUBSPOT_FORM_ID=

# Google Maps JavaScript API Key (optional, if using JavaScript API)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=

# Google My Maps Map ID (for iframe embedding)
# Get this from your Google My Maps embed URL: https://www.google.com/maps/d/embed?mid=YOUR_MAP_ID
NEXT_PUBLIC_GOOGLE_MAPS_ID=1FjdzczdBlhZ5gV1nAh7IBLpomib8Abg

